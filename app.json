{
  "id": "nl.xseth.wallbox",
  "version": "0.1.0",
  "compatibility": ">=5.0.0",
  "sdk": 3,
  "name": {
    "en": "Wallbox Charger"
  },
  "brandColor": "#60b59d",
  "description": {
    "en": "Adds support for the Wallbox Charger"
  },
  "category": [
    "energy"
  ],
  "permissions": [],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Roy K",
    "website": "https://github.com/rkokkelk/nl.xseth.wallbox"
  },
  "bugs": {
    "url": "https://github.com/rkokkelk/nl.xseth.wallbox/issues"
  },
  "source": "https://github.com/rkokkelk/nl.xseth.dashboardctrl",
  "contributors": {
    "developers": [
      {
        "name": "rkokkelk",
        "github": "https://github.com/rkokkelk/"
      }
    ]
  },
  "flow" : {
    "triggers": [
      {
            "id": "charging_finished",
            "title": {
              "en": "Finished charging",
              "nl": "Laden afgerond"
            }
      },
      {
            "id": "charging_ended",
            "title": {
              "en": "Charging ended",
              "nl": "Laden beÃ«indigd"
            }
      },
      {
            "id": "charging_started",
            "title": {
              "en": "Charging started",
              "nl": "Laden gestart"
            }
      },
      {
            "id": "car_connected",
            "title": {
              "en": "Car connected",
              "nl": "Auto verbonden"
            }
      },
      {
            "id": "car_unplugged",
            "title": {
              "en": "Car unplugged",
              "nl": "Auto ontkoppeld"
            }
      },
      {
            "id": "charging_allowed",
            "title": {
              "en": "Charging allowed",
              "nl": "Opladen toegestaan"
            }
      },
      {
            "id": "charging_disallowed",
            "title": {
              "en": "Charging disallowed",
              "nl": "Opladen niet meer toegestaan"
            }
      },
      {
            "id": "status_changed",
            "title": {
              "en": "Status changed",
              "nl": "Status gewijzigd"
            }
      }
    ],
    "conditions" : [
      {
            "id": "is_finished",
            "title": {
              "en": "!{{Finished charging|Not finished charging}}",
              "nl": "!{{Laden afgerond|Laden niet afgerond}}"
            }
      },
      {
            "id": "is_charging",
            "title": {
              "en": "!{{Charging|Not charging}}",
              "nl": "!{{Bezig met laden|Niet bezig met laden}}"
            }
      },
      {
            "id": "is_allowed",
            "title": {
              "en": "!{{Charging allowed|Charging not allowed}}",
              "nl": "!{{Laden toegestaan|Laden niet toegestaan}}"
            }
      },
      {
            "id": "is_plugged",
            "title": {
              "en": "!{{Car plugged in|No car plugged in}}",
              "nl": "!{{Auto verbonden|Geen auto verbonden}}"
            }
      }
    ],
    "actions" : [
      {
            "id": "Allow_charging",
            "title": {
              "en": "Allow charging",
              "nl": "Laden toestaan"
            }
      },
      {
            "id": "Stop_charging",
            "title": {
              "en": "Stop charging",
              "nl": "Laden stoppen"
            }
      },
      {
            "id": "Change_amps_to",
            "title": {
              "en": "Change amperage to...",
              "nl": "Amperage veranderen naar..."
            },
            "titleformatted": {
              "en": "Change amperage to [[amps]]",
              "nl": "Amperage veranderen naar [[amps]]"
            },
            "args": [
              {
                "type": "range",
                "name": "amps",
                "min": 6,
                "max": 32,
                "step": 1,
                "label": "A",
                "labelMultiplier": 1,
                "labelDecimals": 0
              }
            ]
      }
    ]
  },
  "capabilities": {
    "pluggedin": {
      "type": "boolean",
      "title": {
        "en": "Car plugged in",
        "nl": "Auto verbonden"
      },
      "getable": true,
      "setable": false,
      "uiComponent": null,
      "insights": false
    },
    "charging": {
      "type": "boolean",
      "title": {
        "en": "Charging",
        "nl": "Laden"
      },
      "getable": true,
      "setable": false,
      "uiComponent": null,
      "insights": false
    },
    "onoff": {
      "type": "boolean",
      "title": {
        "en": "Charging allowed",
        "nl": "Laden toegestaan"
      },
      "getable": true,
      "setable": true,
      "uiQuickAction": true,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "measure_power": {
      "type": "number",
      "title": {
        "en": "Current Power Supplied",
        "nl": "Actueel wattage"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "W"
      },
      "min": 0,
      "max": 22000,
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "measure_current": {
      "type": "number",
      "title": {
        "en": "Current Amperage Supplied",
        "nl": "Actueel amperage"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "A"
      },
      "min": 0,
      "max": 100,
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "measure_voltage": {
      "type": "number",
      "title": {
        "en": "Current Voltage Supplied",
        "nl": "Actueel voltage"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "V"
      },
      "min": 0,
      "max": 750,
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "measure_temperature": {
      "type": "number",
      "title": {
        "en": "Temperature",
        "nl": "Temperatuur"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "C"
      },
      "min": -50,
      "max": 50,
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "meter_power": {
      "type": "number",
      "title": {
        "en": "Power supplied in this session",
        "nl": "Vermogen in deze laadsessie geleverd"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "kWh"
      },
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "charge_amp": {
      "type": "number",
      "title": {
        "en": "Maximum power from source available for charging",
        "nl": "Maximaal bronvermogen beschikbaar voor laden"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "A"
      },
      "min": 6,
      "max": 32,
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "charge_amp_limit": {
      "type": "number",
      "title": {
        "en": "Maximum power as set in the device's app",
        "nl": "Maximaal vermogen zoals ingesteld in de app van het apparaat"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "A"
      },
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "status": {
      "type": "enum",
      "title": {
        "en": "Device status",
        "nl": "Status van het apparaat"
      },
      "desc": {
        "en": "Device status",
        "nl": "Status van het apparaat"
      },
      "values": [
        {
          "id": "No car connected",
          "title": {
            "en": "No car connected",
            "nl": "Geen auto verbonden"
          }
        },
        {
          "id": "Car connected",
          "title": {
            "en": "Getting ready to start charging",
            "nl": "Klaarmaken voor laden"
          }
        },
        {
          "id": "Charging car",
          "title": {
            "en": "Charging car",
            "nl": "Bezig met laden"
          }
        },
        {
          "id": "Charging finished",
          "title": {
            "en": "Charging finished",
            "nl": "Klaar met laden"
          }
        }
      ],
      "setable": false,
      "getable": true,
      "units": {
        "en": "A"
      },
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "old_status": {
      "type": "enum",
      "uiComponent": null,
      "title": {
        "en": "Device status",
        "nl": "Status van het apparaat"
      },
      "desc": {
        "en": "Previous device status",
        "nl": "Vorige Status van het apparaat"
      },
      "values": [
        {
          "id": "No car connected",
          "title": {
            "en": "No car connected",
            "nl": "Geen auto verbonden"
          }
        },
        {
          "id": "Car connected",
          "title": {
            "en": "Getting ready to start charging",
            "nl": "Klaarmaken voor laden"
          }
        },
        {
          "id": "Charging car",
          "title": {
            "en": "Charging car",
            "nl": "Bezig met laden"
          }
        },
        {
          "id": "Charging finished",
          "title": {
            "en": "Charging finished",
            "nl": "Klaar met laden"
          }
        }
      ],
      "getable": false,
      "setable": false,
      "units": {
        "en": "A"
      },
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "errr": {
      "type": "boolean",
      "title": {
        "en": "Errorstatus",
        "nl": "Foutstatus"
      },
      "getable": true,
      "setable": false,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "energy_total": {
      "type": "number",
      "title": {
        "en": "Total Energy Supplied",
        "nl": "Totaal geleverde energie"
      },
      "getable": true,
      "setable": false,
      "uiComponent": null,
      "units": {
        "en": "kWh"
      },
      "icon": "/assets/icon.svg",
      "insights": true
    }
  },
  "drivers": [
    {
      "id": "wallbox",
      "name": {
        "en": "Wallbox Charger"
      },
      "class": "socket",
      "capabilities": [
        "locked"
      ],
      "images": {
        "large": "/drivers/go-e_charger_home_plus/assets/images/large.png",
        "small": "/drivers/go-e_charger_home_plus/assets/images/small.png"
      },
      "icon":"/drivers/go-e_charger_home_plus/assets/icon.svg",
      "pair": [
          {
            "id": "login_credentials",
            "template": "login_credentials",
            "options": {
              "logo": "icon.png",
              "usernameLabel": { "en": "E-mail address" },
              "usernamePlaceholder": { "en": "john@doe.com" },
              "passwordLabel": { "en": "Password" },
              "passwordPlaceholder": { "en": "Password" }
            }
          },
          {
            "id": "list_devices",
            "template": "list_devices",
            "navigation": { "next": "add_devices" },
            "options": { "singular": true }
          },
          {
            "id": "add_devices",
            "template": "add_devices"
          }
      ]
    }
  ]
}
