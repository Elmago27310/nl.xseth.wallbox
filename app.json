{
  "id": "com.athom.goecharger",
  "version": "0.0.1",
  "compatibility": ">=1.5.0",
  "sdk": 2,
  "name": {
    "en": "Go-e Charger"
  },
  "description": {
    "en": "Adds support for the Go-e car Charger"
  },
  "category": [
    "energy"
  ],
  "permissions": [],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Tim Willemsen",
    "email": "tim.willemsen@gmail.com"
  },
  "contributors": {
    "developers": [
      {
        "name": "abaretta",
        "github": "https://github.com/abaretta"
      },
      {
        "name": "biemond",
        "github": "https://github.com/biemond"
      },
      {
        "name": "Stefan Simon",
        "gitlab": "https://gitlab.com/StefanSimon"
      }
    ]
  },
  "contributing": {
    "donate": {
      "paypal": {
        "username": "paypal@tim-w.com"
      }
    }
  },
  "capabilities": {
    "onoff": {
      "type": "boolean",
      "title": {
        "en": "Allow charging",
        "nl": "Laden toegestaan"
      },
      "getable": true,
      "setable": true,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "measure_power": {
      "type": "number",
      "title": {
        "en": "Current Power Supplied",
        "nl": "Actueel wattage"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "kW"
      },
      "min": 0,
      "max": 22,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "measure_current": {
      "type": "number",
      "title": {
        "en": "Current Amperage Supplied",
        "nl": "Actueel amperage"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "A"
      },
      "min": 0,
      "max": 100,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "measure_voltage": {
      "type": "number",
      "title": {
        "en": "Current Voltage Supplied",
        "nl": "Actueel voltage"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "V"
      },
      "min": 0,
      "max": 750,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "measure_temperature": {
      "type": "number",
      "title": {
        "en": "Temperature",
        "nl": "Temperatuur"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "C"
      },
      "min": -50,
      "max": 50,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "meter_power": {
      "type": "number",
      "title": {
        "en": "Power supplied in this session",
        "nl": "Vermogen in deze laadsessie geleverd"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "kWh"
      },
      "icon": "/assets/icon.svg",
      "insights": true
    },
    "charge_amp": {
      "type": "number",
      "title": {
        "en": "Maximum power from source available for charging",
        "nl": "Maximaal bronvermogen beschikbaar voor laden"
      },
      "getable": true,
      "setable": true,
      "units": {
        "en": "A"
      },
      "min": 6,
      "max": 32,
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "charge_amp_limit": {
      "type": "number",
      "title": {
        "en": "Maximum power as set in the device's app",
        "nl": "Maximaal vermogen zoals ingesteld in de app van het apparaat"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "A"
      },
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "status": {
      "type": "enum",
      "title": {
        "en": "Device status",
        "nl": "Status van het apparaat"
      },
      "desc": {
        "en": "Device status",
        "nl": "Status van het apparaat"
      },
      "values": [
        {
          "id": "Ready to connect car",
          "title": {
            "en": "Ready to connect car",
            "nl": "Klaar om auto te koppelen"
          }
        },
        {
          "id": "Charging car",
          "title": {
            "en": "Charging car",
            "nl": "Auto laden"
          }
        },
        {
          "id": "Car connected, waiting for car",
          "title": {
            "en": "Car connected, waiting for car",
            "nl": "Auto verbonden, wacht op auto"
          }
        },
        {
          "id": "Charging finished",
          "title": {
            "en": "Charging finished",
            "nl": "Klaar met laden"
          }
        }
      ],
      "getable": true,
      "setable": false,
      "units": {
        "en": "A"
      },
      "icon": "/assets/icon.svg",
      "insights": false
    },
    "errr": {
      "type": "boolean",
      "title": {
        "en": "Errorstatus",
        "nl": "Foutstatus"
      },
      "getable": true,
      "setable": false,
      "icon": "/assets/icon.svg",
      "insights": false
    }
  },
  "drivers": [
    {
      "id": "go-e_charger_home_plus",
      "name": {
        "en": "Go-e Charger Home+"
      },
      "class": "socket",
      "capabilities": [
        "onoff",
        "measure_power",
        "measure_current",
        "measure_voltage",
        "measure_temperature",
        "meter_power",
        "status",
        "errr",
        "charge_amp",
        "charge_amp_limit"
      ],
      "images": {
        "large": "/drivers/go-e_charger_home_plus/assets/images/large.png",
        "small": "/drivers/go-e_charger_home_plus/assets/images/small.png"
      },
      "icon":"/drivers/go-e_charger_home_plus/assets/icon.svg",
      "pair": [
        {
          "id": "start"
        }
      ]
    }
  ]
}
